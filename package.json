{
  "name": "tree-sitter-nexus",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for the Nexus programming language",
  "main": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "lexer",
    "nexus",
    "syntax"
  ],
  "author": "Xyndra",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.18.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.22.6"
  },
  "scripts": {
    "build": "tree-sitter generate",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  },
  "tree-sitter": [
    {
      "scope": "source.nexus",
      "injection-regex": "^nexus$",
      "file-types": [
        "nx"
      ],
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm"
    }
  ]
}
